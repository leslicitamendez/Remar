<h1>Listing conyugues</h1>

<table>
  <thead>
    <tr>
    <th>interno_id</th>
      <th>Estado</th>
      <th>Nombre</th>
      <th>Apellido1</th>
      <th>Apellido2</th>
      <th>Ci</th>
      <th>Lugarnacimiento</th>
      <th>Fechanacimiento</th>
      <th>Telefono</th>
      <th>Direccion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @conyugues.each do |conyugue| %>
      <tr>
        <td><%= conyugue.interno_id%></td>
        <td><%= conyugue.estado %></td>
        <td><%= conyugue.nombre %></td>
        <td><%= conyugue.apellido1 %></td>
        <td><%= conyugue.apellido2 %></td>
        <td><%= conyugue.ci %></td>
        <td><%= conyugue.lugarNacimiento %></td>
        <td><%= conyugue.fechaNacimiento %></td>
        <td><%= conyugue.telefono %></td>
        <td><%= conyugue.direccion %></td>
        <td><%= link_to 'Show', conyugue %></td>
        <td><%= link_to 'Edit', edit_conyugue_path(conyugue) %></td>
        <td><%= link_to 'Destroy', conyugue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

