<center>
  <p id="notice"><%= notice %></p>

  <h1>Egresos de <%= @egreso_interno.interno.nombre %> </h1><br>
  <div align="right"><%= link_to 'Internos', internos_path, :class => 'btn btn-mini btn-default' %> | 
    <%= link_to 'Atras', :back, :class => 'btn btn-mini btn-default' %></div>

  <p>
    <strong>Concepto:</strong>
    <%= @egreso_interno.concepto %>
  </p>

  <p>
    <strong>Monto Bs.:</strong>
    <%= @egreso_interno.monto %>
  </p>

  <p>
    <strong>Observaciones:</strong>
    <%= @egreso_interno.observaciones %>
  </p>

  <p>
    <strong>Fecha:</strong>
    <%= @egreso_interno.fecha.strftime("%d/%m/%Y") %>
  </p>
</center>
