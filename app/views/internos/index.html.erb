<h1>Listing internos</h1>

<table border="1">
  <thead>
    <tr>
      <th>Fechaingreso</th>
      <th>Nombres</th>
      <th>Apellidopaterno</th>
      <th>Apellidomaterno</th>
      <th>Ci</th>
      <th>Estadocivil</th>
      <th>Edad</th>
      <th>Conyugue</th>
      <th>Numerodhijos</th>
      <th>Lugardnacimiento</th>
      <th>Fechadnacimiento</th>
      <th>Direccion</th>
      <th>Zona</th>
      <th>Telefono</th>
    
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @internos.each do |interno| %>
      <tr>
        <td><%= interno.fechaIngreso %></td>
        <td><%= interno.nombres %></td>
        <td><%= interno.apellidoPaterno %></td>
        <td><%= interno.apellidoMaterno %></td>
        <td><%= interno.ci %></td>
        <td><%= interno.estadoCivil %></td>
        <td><%= interno.edad %></td>
        <td><%= interno.conyugue %></td>
        <td><%= interno.numeroDHijos %></td>
        <td><%= interno.lugarDNacimiento %></td>
        <td><%= interno.fechaDNacimiento %></td>
        <td><%= interno.direccion %></td>
        <td><%= interno.zona %></td>
        <td><%= interno.telefono %></td>

        <td><%= link_to 'Show', interno %></td>
        <td><%= link_to 'Edit', edit_interno_path(interno) %></td>
        <td><!--<%= link_to 'Destroy', interno, method: :delete, data: { confirm: 'Are you sure?' } %>--></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interno', new_interno_path %>
