<h1>Listing court_orders</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Descripcion</th>
      <th>Estado</th>
     
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @court_orders.each do |court_order| %>
      <tr>
        <td><%= court_order.descripcion %></td>
        <td><%= court_order.estado %></td>
        
        <td><%= link_to 'Show', court_order %></td>
        <td><%= link_to 'Edit', edit_court_order_path(court_order) %></td>
        <td><%= link_to 'Destroy', court_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Court order', new_court_order_path %>
