<h1>Listing ventaprods</h1>

<table>
  <thead>
    <tr>
      <th>Cantidad</th>
      <th>Fecha</th>
      <th>Preciounidad</th>
      <th>Estado</th>
      <th>Observaciones</th>
      <th>Rendicionprod</th>
      <th>Entregaprod</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ventaprods.each do |ventaprod| %>
      <tr>
        <td><%= ventaprod.cantidad %></td>
        <td><%= ventaprod.fecha %></td>
        <td><%= ventaprod.precioUnidad %></td>
        <td><%= ventaprod.estado %></td>
        <td><%= ventaprod.observaciones %></td>
        <td><%= ventaprod.rendicionprod %></td>
        <td><%= ventaprod.entregaprod %></td>
        <td><%= link_to 'Show', ventaprod %></td>
        <td><%= link_to 'Edit', edit_ventaprod_path(ventaprod) %></td>
        <td><%= link_to 'Destroy', ventaprod, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ventaprod', new_ventaprod_path %>
